
{
    "manifest_version": 2,
    "name": "Twim",
    "version": "0.2.4",
    "description": "a twitter image downloader chrome extension",
    "icons": {
        "128": "icon-enable.png"
    },
    "content_scripts": [{
        "matches": ["https://twitter.com/*"],
        "js": ["twim-loader.js", "twim.js"]
    }],
    "browser_action": {
        "default_icon": "icon-disable.png"
    },
    "background": {
        "scripts": ["twim-background.js"]
    },
    "permissions": ["tabs", "background", "downloads", "https://twitter.com/*"],
    "options_page": "twim-options.html",
    "web_accessible_resources": ["icon-enable.png"]
}
